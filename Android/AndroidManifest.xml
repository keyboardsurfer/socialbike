<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.cbase.dev.adkbike" android:versionCode="1"
	android:versionName="1.0">
	<application android:label="@string/app_name" android:icon="@drawable/icon">
		<activity android:name=".SocialBikeActivity">
			<uses-library android:name="com.android.future.usb.accessory" />
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="UsbAccessoryActivity" android:label="DemoKit"
			android:taskAffinity="" android:launchMode="singleInstance">
			<intent-filter>
				<action android:name="android.hardware.usb.action.USB_ACCESSORY_ATTACHED" />
			</intent-filter>

			<meta-data android:name="android.hardware.usb.action.USB_ACCESSORY_ATTACHED"
				android:resource="@xml/accessory_filter" />
		</activity>
	</application>
</manifest> 
